            "rsync://mirror.hackingand.coffee/arch/$repo/os/$arch",
            "rsync://dfw.mirror.rackspace.com/archlinux/$repo/os/$arch",
            "rsync://mirrors.lug.mtu.edu/archlinux/$repo/os/$arch",
